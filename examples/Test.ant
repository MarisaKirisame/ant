let test_match = fun x ->
  match x with
  | Nil -> 0
  | Cons a b -> 1;;

let test_match2 = fun x ->
  match x with
  | Pair a b -> a + b;;

let test_wrap = fun x ->
  match x with
  | Wrap a -> a;;

