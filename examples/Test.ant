type list 'a =
  | Nil
  | Cons of 'a * list 'a;;

let nil = fun _ -> Nil;;

let cons = fun a b -> Cons a b;;

let test_op = fun a b -> a + b;;

let test_const_op = fun _ ->
  let x = 1 + 2 in
  x;;

let test_nested_let = fun x ->
  let y = (let z = 1 + 2 in z) in
  y;;

let test_if = fun x ->
  if x then 1 else 2;;

let test_nested_if = fun x ->
  if (if x then 1 else 0) < 1 then 1 else 2;;

